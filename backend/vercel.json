{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/darko/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "backend/api/index.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "frontend/darko/dist/$1" }
  ]
}
